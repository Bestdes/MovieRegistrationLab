<h2>Registration for New Movies</h2>

<form action="Result2" method="post">
    <p>ID</p>
    <input type="number" maxlength="1" name="ID" />

    <p>Title</p>
    <input type="text" name="Title" />

    <p>Genre</p>
    <input type="text" name="Genre" />

    <p>Year</p>
    <input type="number" name="Year" />

    <p>Actors</p>
    <input type="text" name="Actors" />

    <p>Directors</p>
    <input type="text" name="Directors" />

    <input type="submit" />
</form>
